                    </a>
                    <span class=" icon-thumbnail"><i class="pg-home"></i></span>
                </li>
                <li class="">
                    <a href="@Url.RouteUrl("category")" class="detailed">
                        <span class="title">Category</span>
                        @*<span class="details">22 items</span>*@
                    </a>
                    <span class="icon-thumbnail"><i class="pg-layouts2"></i></span>
                </li>
                <li class="">
                    <a href="@Url.RouteUrl("user")" class="detailed">
                        <span class="title">User</span>
                        @*<span class="details">22 items</span>*@
                    </a>
                    <span class="icon-thumbnail"><i class="fa fa-user"></i></span>
                </li>
                <li class="">
                    <a href="@Url.RouteUrl("event")" class="detailed">
