                        @*<span class="details">22 items</span>*@
                    </a>
                    <span class="icon-thumbnail"><i class="fa fa-user"></i></span>
                </li>
                <li class="">
                    <a href="@Url.RouteUrl("calling")" class="detailed">
                        <span class="title">Calling List</span>
                        @*<span class="details">22 items</span>*@
                    </a>
                    <span class="icon-thumbnail"><i class="fa fa-user"></i></span>
                </li>
				 <li class="">
                    <a href="@Url.RouteUrl("callingprices")" class="detailed">
                        <span class="title">Calling Price</span>
                        @*<span class="details">22 items</span>*@
                    </a>
                    <span class="icon-thumbnail"><i class="fa fa-user"></i></span>
                </li>
                
                <li class="">
                    <a href="@Url.RouteUrl("EventCategoryimage")" class="detailed">
                        <span class="title">Event Category image</span>
                        @*<span class="details">22 items</span>*@
                    </a>
                    <span class="icon-thumbnail"><i class="fa fa-user"></i></span>
                </li>
                <li>
