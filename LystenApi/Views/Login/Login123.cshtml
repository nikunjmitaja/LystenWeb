                    <a href="javascript:void(0);">  <p class="short_desc forgot_pwd_btn text-white m_t_20 col-md-5  text-right">Forgot Password?</p></a>
                    <div class="form-group m_t_20 col-md-12 ">
                        @*<a  onclick="return CheckLogin()" href="#" class="log_btn">Sign in</a>*@

                        <button id="btnLogin" onclick="return CheckLogin()" type="submit" class="log_btn">
                            Sign in
                        </button>

                    </div>
                </div>
            }
            <div class="col-md-8 col-md-offset-2 login_portion reg ">
                <div class="form-group col-md-4">
                    @*<input type="text" placeholder="User Name" class="form-control">*@
                    <input type="text" id="txtEmail" class="form-control custom required" value="" placeholder="Email Address">
